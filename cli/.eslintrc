{
    "extends": [
        "airbnb-base",
        "airbnb-typescript/base",
        "../.eslintrc"
    ],
    "parserOptions": {
        "project": "cli/tsconfig.json"
    },
    "ignorePatterns": [
        "**/*.d.*",
        "templates/**.*",
        "**/*.cjs",
        "**/*.js",
        "e2e/**/*",
        "scripts/*.ts"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-console": "off",
        "consistent-return": "off",
        "class-methods-use-this": "off",
        "@typescript-eslint/lines-between-class-members": "off"
    }
}