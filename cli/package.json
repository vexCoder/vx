{
  "name": "cli",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "initializer": "rimraf dist/init && cpy ../init !../init/node_modules dist/init --parents --dot",
    "templates": "rimraf dist/templates && cpy ../templates !../templates/**/node_modules dist/templates --parents --dot",
    "build": "tsc && yarn templates && yarn initializer",
    "cli": "node --no-warnings --loader ts-node/esm src/index.ts",
    "dev:cli": "nodemon dist/index.js",
    "prewatch": "yarn templates && yarn initializer",
    "watch": "tsc -w --preserveWatchOutput",
    "pre:dev": "rimraf example && mkdir example",
    "test-run": "node --no-warnings --loader ts-node/esm test/manual/run.ts",
    "test": "ava --no-worker-threads --timeout=120s",
    "circular": "madge --circular src/index.ts"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/ejs": "^3.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.1.3",
    "@types/lodash": "^4.14.178",
    "@types/minimatch": "^3.0.5",
    "@types/node": "^17.0.6",
    "@types/rimraf": "^3.0.2",
    "@types/verror": "^1.10.5",
    "ava": "^4.0.1",
    "concurrently": "^7.0.0",
    "cpy-cli": "^3.1.1",
    "execa": "^6.0.0",
    "findit2": "^2.2.3",
    "lodash": "^4.17.21",
    "madge": "^5.0.1",
    "minimatch": "^3.0.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0"
  },
  "dependencies": {
    "chalk": "^5.0.0",
    "cli-spinners": "^2.6.1",
    "consola": "^2.15.3",
    "ejs": "^3.1.6",
    "enquirer": "^2.3.6",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.0.0",
    "globby": "^13.1.1",
    "inquirer": "^8.2.0",
    "lodash": "^4.17.21",
    "meow": "^10.1.2",
    "ora": "^6.1.0",
    "p-map": "^5.3.0",
    "read-pkg-up": "^9.0.0",
    "rimraf": "^3.0.2",
    "table": "^6.8.0",
    "type-fest": "^2.12.0",
    "verror": "^1.10.1"
  },
  "main": "index.js",
  "exports": "./src/index.ts",
  "directories": {
    "example": "example",
    "test": "test"
  }
}
