{
  "name": "cli",
  "version": "3.0.10",
  "type": "module",
  "description": "CLI manager for vex-turbo-boilerplate",
  "author": "vexCoder <freelance.starterpack08@gmail.com>",
  "scripts": {
    "initializer": "rimraf dist/init && cpy ../init !../init/node_modules dist/init --parents --dot",
    "templates": "rimraf dist/templates && cpy ../templates !../templates/**/node_modules dist/templates --parents --dot",
    "build": "tsc && yarn templates && yarn initializer",
    "cli": "node --no-warnings --loader ts-node/esm src/index.ts",
    "dev:cli": "nodemon dist/index.js",
    "prewatch": "yarn templates && yarn initializer",
    "watch": "tsc -w --preserveWatchOutput",
    "pre:dev": "rimraf example && mkdir example",
    "test-run": "node --no-warnings --loader ts-node/esm test/manual/run.ts",
    "test": "ava --no-worker-threads --timeout=120s",
    "circular": "madge --circular src/index.ts"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "bugs": {
    "url": "https://github.com/vexCoder/vx/issues"
  },
  "homepage": "https://github.com/vexCoder/vx#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/vexCoder/vx.git"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/color": "^3.0.3",
    "@types/dedent": "^0.7.0",
    "@types/ejs": "^3.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.1.3",
    "@types/lodash": "^4.14.178",
    "@types/minimatch": "^3.0.5",
    "@types/node": "^17.0.6",
    "@types/rimraf": "^3.0.2",
    "ava": "^4.0.1",
    "concurrently": "^7.0.0",
    "cpy-cli": "^3.1.1",
    "execa": "^6.0.0",
    "findit2": "^2.2.3",
    "madge": "^5.0.1",
    "minimatch": "^3.0.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0"
  },
  "dependencies": {
    "chalk": "^5.0.0",
    "cli-spinners": "^2.6.1",
    "color": "^4.2.1",
    "consola": "^2.15.3",
    "dedent": "^0.7.0",
    "ejs": "^3.1.6",
    "enquirer": "^2.3.6",
    "fast-glob": "^3.2.11",
    "figures": "^4.0.1",
    "fs-extra": "^10.0.0",
    "globby": "^13.1.1",
    "ink": "^3.2.0",
    "inquirer": "^8.2.0",
    "lodash": "^4.17.21",
    "meow": "^10.1.2",
    "p-map": "^5.3.0",
    "react": "^17.0.2",
    "read-pkg-up": "^9.0.0",
    "rimraf": "^3.0.2",
    "type-fest": "^2.12.0",
    "valtio": "1.3.1"
  },
  "resolutions": {
    "valtio": "1.3.1"
  },
  "main": "index.js",
  "exports": "./src/index.ts",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "vx": "dist/index.js"
  },
  "gitHead": "2b53c0a5739a76a2085a1223afd662342b09c1bd"
}
